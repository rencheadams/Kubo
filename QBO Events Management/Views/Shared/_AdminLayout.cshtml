<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>QBO - @ViewBag.Title</title>
    <link rel="stylesheet" href="" />
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/additional/qboAdmin.css")
    @Scripts.Render("~/bundles/modernizr")
    @using Microsoft.AspNet.Identity

</head>
<body>


    
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <img src="@Url.Content("~/Content/img/logo.png")" class="img-responsive logo" alt="Image" />
                @*@Html.ActionLink("Application name", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })*@
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Events", "ListOfEvents", "Events")</li>
                    <li>@Html.ActionLink("HQ", "Index", "HQ")</li>
                    <li>@Html.ActionLink("People", "Index", "People")</li>
                    @*<li>@Html.ActionLink("Contact", "Contact", "Home")</li>*@
                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>



    <div class="">
        @*  body-content *@

        @RenderBody()

    </div>



    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
